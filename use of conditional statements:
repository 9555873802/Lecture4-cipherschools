#include <iostream>

int main() {
    int x = 10;
    int y = 20;

    // if statement
    if (x > y) {
        std::cout << "x is greater than y" << std::endl;
    }

    // if-else statement
    if (x > y) {
        std::cout << "x is greater than y" << std::endl;
    } else {
        std::cout << "y is greater than x" << std::endl;
    }

    // nested if statement
    if (x == y) {
        std::cout << "x and y are equal" << std::endl;
    } else {
        if (x > y) {
            std::cout << "x is greater than y" << std::endl;
        } else {
            std::cout << "y is greater than x" << std::endl;
        }
    }

    return 0;
}
